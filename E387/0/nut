FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;

}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1e-10;

boundaryField
{
    in
    {
        type            calculated;
        value           uniform 0;
    }
    out
    {
        type            calculated;
        value           uniform 0;
    }
    w_dw
    {
        type            fixedValue;
        value           uniform 1.0e-10;
    }
    w_up
    {
        type            fixedValue;
        value           uniform 1.0e-10;
    }
    empty
    {
        type            empty;
    }
}


