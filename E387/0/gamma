FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      gamma;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    in
    {
	type		fixedValue;
	value		uniform 1;
    }
    out
    {
        type            zeroGradient;
    }
    w_dw
    {
        type            zeroGradient;
    }
    w_up
    {
        type            zeroGradient;
    }
    empty
    {
        type            empty;
    }
}


