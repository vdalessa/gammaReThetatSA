FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     simpleFoam;

startFrom       latestTime;

startTime       0;

//stopAt          writeNow;
stopAt          endTime;

endTime         5000;

deltaT          1;

writeControl    timeStep;

writeInterval   1000;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

graphFormat     raw;

libs ("libUnivpmRASModels.so");

functions
{
   forces
   {
          type forces;
          functionObjectLibs ("libforces.so");
          patches (w_dw w_up);
          pName p;
          UName U;
          rhoName rhoInf;
          rhoInf 1;
          CofR (0.25 0 0); // Origin for moment calculations
          outputControl timeStep;
          outputInterval 1;
   }
//
